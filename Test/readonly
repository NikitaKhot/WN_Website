    <script>

        $('.hamburger-button').click(function () {
            var clicks = $(this).data('clicks');
            if (clicks) {
                console.log('open');
                $('body').removeClass('no-scroll')
            } else {
                console.log('close');
                $('body').toggleClass('no-scroll')
            }
            $(this).data("clicks", !clicks);
        });

    </script>

